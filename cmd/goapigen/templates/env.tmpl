# Server configuration
PORT={{.DefaultPort}}
ENV=development

# MongoDB configuration
MONGO_URI={{.MongoURI}}
DB_NAME={{.DBName}}

# JWT Settings (if needed)
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRATION=24h

# CORS Settings
CORS_ALLOWED_ORIGINS=*

# API versioning
API_VERSION=v1

# Logging
LOG_LEVEL=debug 